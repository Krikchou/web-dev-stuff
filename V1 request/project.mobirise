          "_name": "contact3",
          "_customHTML": "<section class=\"contact3\" group=\"Contacts\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Height Content\" name=\"heightContent\" min=\"400\" max=\"700\" step=\"20\" value=\"480\">\n        <input type=\"checkbox\" title=\"Left/Right Content\" name=\"reverse\">\n        <header>Map</header>\n        <input type=\"map\" title=\"Map\" name=\"googleMap\" place-id=\"sofia, bulgaria\" value=\"sofia, bulgaria\">\n        <header>Content</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#ffffff\" name=\"borderColor\">\n        <input type=\"color\" title=\"Block Color 1\" value=\"#111111\" name=\"blockColor1\">\n        <input type=\"color\" title=\"Block Color 2\" value=\"#000000\" name=\"blockColor2\">\n        <select name=\"alignContent\" title=\"Align Content\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"container-fluid\">\n        <div class=\"row main\">\n            <div class=\"col-md-6 map\">\n              <div class=\"google-map\" mbr-map=\"googleMap\"></div>\n            </div>\n            <div class=\"col-md-6 text-white wrap__content\">\n                    <div class=\"col-md-6 col-sm-6 block__content1\" mbr-class=\"{'align-left' : alignContent == 1, 'align-center' : alignContent == 2, 'align-right' : alignContent == 3}\">\n                        <div class=\"d-flex icon