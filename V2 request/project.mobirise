          "_customHTML": "<!-- Type valid HTML here -->\n\n  <div class=\"sidenav\">\n        <div class=\"login-main-text\">\n           <h2>Application<br> Login Page</h2>\n  \n        </div>\n     </div>\n     <div class=\"main\">\n        <div class=\"col-md-6 col-sm-12\">\n           <div class=\"login-form\">\n              <form>\n                 <div class=\"form-group\">\n                    <label>User Name</label>\n                    <input type=\"text\" class=\"form-control\" placeholder=\"User Name\">\n                 </div>\n                 <div class=\"form-group\">\n                    <label>Password</label>\n                    <input type=\"password\" class=\"form-control\" placeholder=\"Password\">\n                 </div>\n                 <button type=\"submit\" class=\"btn btn-secondary\">Login</button>\n           \n              </form>\n           </div>\n        </div>\n     </div>",
          "_customCSS": "body {\n    font-family: \"Lato\", sans-serif;\n}\n\n\n\n.main-head{\n    height: 150px;\n    background: #FFF;\n   \n}\n\n.sidenav {\n    height: 100%;\n    background-color: orangered;\n    overflow-x: hidden;\n    padding-top: 20px;\n}\n\n\n.main {\n    padding: 0px 10px;\n}\n\n@media screen and (max-height: 450px) {\n    .sidenav {padding-top: 15px;}\n}\n\n@media screen and (max-width: 450px) {\n    .login-form{\n        margin-top: 10%;\n    }\n\n    .register-form{\n        margin-top: 10%;\n    }\n}\n\n@media screen and (min-width: 768px){\n    .main{\n        margin-left: 40%; \n    }\n\n    .sidenav{\n        width: 40%;\n        position: fixed;\n        z-index: 1;\n        top: 0;\n        left: 0;\n    }\n\n    .login-form{\n        margin-top: 80%;\n    }\n\n    .register-form{\n        margin-top: 20%;\n    }\n}\n\n\n.login-main-text{\n    margin-top: 20%;\n    padding: 60px;\n    color: #fff;\n}body {\n    font-family: \"Lato\", sans-serif;\n}\n\n\n\n.main-head{\n    height: 150px;\n    background: #FFF;\n   \n}\n\n.sidenav {\n    height: 100%;\n    background-color: rgb(184, 54, 8);\n    overflow-x: hidden;\n    padding-top: 20px;\n}\n\n\n.main {\n    padding: 0px 10px;\n}\n\n@media screen and (max-height: 450px) {\n    .sidenav {padding-top: 15px;}\n}\n\n@media screen and (max-width: 450px) {\n    .login-form{\n        margin-top: 10%;\n    }\n\n    .register-form{\n        margin-top: 10%;\n    }\n}\n\n@media screen and (min-width: 768px){\n    .main{\n        margin-left: 40%; \n    }\n\n    .sidenav{\n        width: 40%;\n        position: fixed;\n        z-index: 1;\n        top: 0;\n        left: 0;\n    }\n\n    .login-form{\n        margin-top: 80%;\n    }\n\n    .register-form{\n        margin-top: 20%;\n    }\n}\n\n\n.login-main-text{\n    margin-top: 20%;\n    padding: 60px;\n    color: #fff;}",
